{
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/supertest": "^2.0.13",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "posttest": "npm run lint",
    "connectprisma": "npx prisma generate && npx prisma migrate deploy && npx prisma db seed || exit 0",
    "start": "npm run connectprisma && node build/src/index.js",
    "buildAndStart": "npm run connectprisma && tsc && node build/src/index.js",
    "dev": "nodemon -L -w src -e ts --exec \"npm run buildAndStart\"",
    "test": "jest --runInBand --collect-coverage"
  },
  "prisma": {
    "seed": "ts-node prisma/seed/seedData.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.4.1",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.20",
    "@types/node": "20.4.6",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "gts": "^5.0.1",
    "typescript": "^5.2.2"
  }
}
